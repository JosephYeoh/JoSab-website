{
    "env": {
        "es2020": true,
        "node": true
    },
    "extends": [
        "airbnb-base",
        "prettier"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 11,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint",
        "prettier"
    ],
    "rules": {
        "prettier/prettier": [1],
        "import/prefer-default-export": [0],
        "no-unused-vars": "off",
        "no-empty-pattern": "off",
        "import/no-dynamic-require": [0],
        "global-require":"off"
    }
}
