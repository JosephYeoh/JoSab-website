{
  "presets": ["next/babel"],
  "plugins": [
    ["@babel/plugin-proposal-decorators", { "legacy": true }],
    ["babel-plugin-styled-components", { "ssr": true }],
    [
      "module-resolver",
      {
				"root": ["."],
				"alias": {
						"@common": "../common/src",
						"@static": "../static",
						"src": "./src"
        }
      }
    ]
  ]
}